{	field : '%%FieldNameVal%%',
	title : '%%FieldLabelVal%%',
	width : 120,
	align : 'left' ,
	formatter : function(value,row, index) {
		var result ='';
		var tempValue =row.%%FieldNameVal%%;
		if (tempValue) {	
			result =tempValue;
		}  else { 
			result=  '';
		}
		//if(result.length>20){
		//	result = result.substring(0,20)+'...';
		//}
		return result;
	}	 
} ,